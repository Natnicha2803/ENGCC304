#include <stdio.h>

int main(){
    int N ;
    int number[N] ;
    int i ;
    int array;
    int check ;
    printf("Enter N : \n");
    scanf("%d",&N);

    for ( i = 0; i < N; i++)
    {
       printf("Enter value [%d] : ",i);
       scanf("%d",&number[i]);
    }
    printf("Index: ") ;

    for ( i = 0; i < N; i++)
    {
        printf(" %d ",i);
    }
    printf("\nArray: ");

    for ( check = 2; check  < number[i] ; check++)
    {
       number[i]%check;
       
    }
    printf(" %d ",number[i]);
    return 0 ;
}
